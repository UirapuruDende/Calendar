{
    "name": "dende/calendar",
    "description": "PHP Calendar component",
    "license": "proprietary",
    "require": {
        "nesbot/carbon": "*",
        "doctrine/common": "*",
        "doctrine/orm": "^2.5",
        "humbug/humbug": "~1.0@dev",
        "ramsey/uuid": "^3.6",
        "symfony/event-dispatcher": "^3.2"
    },
    "autoload": {
        "psr-0": {
            "Dende\\Calendar\\": ""
        },
        "psr-4": {
            "Dende\\Calendar\\": ""
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "behat/behat": "^3.0",
        "friendsofphp/php-cs-fixer": "^1.12",
        "phpmd/phpmd": "^2.6"
    },
    "scripts": {
        "humbug" : [
            "./bin/humbug"
        ],
        "phpunit" : [
            "./bin/phpunit"
        ],
        "phpmd" : [
            "./bin/phpmd Domain,Application,Infrastructure text phpmd.xml"
        ],
        "php-cs-fixer" : [
            "./bin/php-cs-fixer fix --dry-run"
        ],
        "test" : [
            "@phpunit",
            "@php-cs-fixer"
        ]
    },
    "authors": [
        {
            "name": "Grzegorz Kaszuba",
            "email": "uirapuruadg@gmail.com"
        }
    ],
    "config": {
        "bin-dir": "bin"
    }
}
